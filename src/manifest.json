{
    "background": {
      "scripts": ["bg.js"]
    },
    "browser_action": {
        "browser_style": false,
        "default_icon": "icons/icon-vec.svg",
        "default_popup": "popup/toggles.html",
        "default_title": "Toggle text contrast fix"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["lib/color.js", "lib/contrast.js", "embed.js"],
        "css": ["fixContrast.css"],
        "run_at": "document_start",
        "all_frames": true
      },
      {
        "matches": ["<all_urls>"],
        "js": ["checkContrast.js"]
      }
    ],
    "description": "Fixes low-contrast text when using system colors with a dark desktop theme.",
    "manifest_version": 2,
    "name": "Text Contrast for Dark Themes",
    "permissions": ["<all_urls>"],
    "version": "1.2.3"
}
