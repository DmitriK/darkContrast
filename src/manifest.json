{
    "author": "Dmitri Kourennyi",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "include_globs": ["*://*"],
        "js": ["checkContrast.js"]
    }],
    "description": "Fixes low-contrast text when using system colors with a dark desktop theme.",
    "gecko": {
        "id": "textcontrast@monochrome101.addons.mozilla.org",
        "strict_min_version": "48.0"
    },
    "homepage": "https://github.com/DmitriK/darkContrast",
    "license": "MIT",
    "manifest_version": 2,
    "name": "textcontrast",
    "title": "Text Contrast for Dark Themes",
    "version": "1.0.0"
}
